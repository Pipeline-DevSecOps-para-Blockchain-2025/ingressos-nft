# WalletConnect Project ID
# Get your project ID from https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Application metadata
VITE_APP_NAME=Ingressos NFT Ticketing
VITE_APP_DESCRIPTION=Web3 NFT Ticketing Platform
VITE_APP_URL=http://localhost:5173
VITE_APP_ICON=http://localhost:5173/vite.svg

# Contract addresses (will be updated after deployment)
VITE_INGRESSOS_CONTRACT_ADDRESS=

# RPC URLs (optional - defaults to public RPCs)
VITE_MAINNET_RPC_URL=
VITE_SEPOLIA_RPC_URL=
VITE_HARDHAT_RPC_URL=http://127.0.0.1:8545

# Option 1: OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-3.5-turbo
# Alternative models: gpt-4, gpt-4-turbo-preview, gpt-3.5-turbo

# Option 2: Google Gemini Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-pro
# Alternative models: gemini-pro-vision, gemini-1.5-pro
# Option 3: Ollama (Local) Configuration

# Install Ollama from: https://ollama.ai
# Then run: ollama pull llama3.2
# No API key needed - runs locally
VITE_OLLAMA_MODEL=llama3.2
VITE_OLLAMA_BASE_URL=http://localhost:11434
